        [ sh:description "\"The time at which performance of the action began.\"" ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path <core:startTime> ],
        [ sh:description "\"The number of times that the action was performed.\"" ;
            sh:maxCount 1 ;
            sh:order 11 ;
